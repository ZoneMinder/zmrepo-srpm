[zmrepo-testing]
name=Zoneminder and its dependencies for Fedora $releasever - $basearch - Test
baseurl=http://zmrepo.mehvc.com/f/testing/$releasever/$basearch
	http://zmrepo.connortechnology.com/f/testing/$releasever/$basearch
#mirrorlist=http://zmrepo.mehvc.com/f/testing/$releasever/$basearch
enabled=0
gpgcheck=1
# zmrepo uses packages from other repos
# Import all required gpg keys
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-free-fedora-$releasever-zmrepo
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zmrepo

